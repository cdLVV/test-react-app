  "private": true,
  "scripts": {
    "prepare": "husky install",
    "check": "npm run static-check && npm run eslint-check && npm run lint:css",
    "static-check": "tsc",
    "eslint-check": "eslint src/ --max-warnings 0",
    "lint:css": "stylelint src/**/*.{css,less} --fix --max-warnings 0",
    "test:e2e": "jest -c jest-e2e.config.js --detectOpenHandles",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start2": "node scripts/start.js",
    "build2": "node scripts/build.js",
    "build": "cross-env NODE_ENV=production BABEL_ENV=production webpack --config ./config/webpack.config.js",
    "analyze": " cross-env ANALYZER=1 NODE_ENV=production BABEL_ENV=production webpack --profile --progress --config ./config/webpack.config.js",
    "start": "cross-env NODE_ENV=development BABEL_ENV=development webpack serve --open --config ./config/webpack.config.js"
  },
